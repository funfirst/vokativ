{
    "name": "bigit/vokativ",
    "description": "Knihovna pro skloňování českých jmen do 5. pádu pro účely generování oslovení.",
    "type": "library",
    "license": "MIT",
    "authors": [{
        "name": "Jiří Žižka",
        "email": "jiri.zizka@funfirst.cz"
    }],
    "require": {
        "php": "^5.5 || ^7.0 || ^8.0"
    },
    "autoload": {
        "psr-4": {
            "Vokativ\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vokativ\\Test\\": "test/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit --bootstrap vendor/autoload.php test"
    }
}